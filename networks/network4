dag {
    Age
    Hypertension
    HeartDisease
    Gender
    BMI
    GlucoseLevel
    Work
    Residence
    Married
    Smoking
    Stroke
    Age -> Hypertension
    Age -> HeartDisease
    Age -> GlucoseLevel
    Age -> BMI
    Age -> Stroke
    Age -> Smoking
    Age -> Married
    Age -> Work
    Gender -> Hypertension
    Gender -> HeartDisease
    Gender -> Work
    Gender -> BMI
    Gender -> Smoking
    Gender -> Stroke
    Gender -> GlucoseLevel
    Hypertension -> HeartDisease
    Hypertension -> Stroke
    HeartDisease -> Stroke
    Smoking -> Hypertension
    Smoking -> HeartDisease
    Smoking -> BMI
    BMI -> Hypertension
    BMI -> HeartDisease
    BMI -> Stroke
    GlucoseLevel -> Hypertension
    GlucoseLevel -> HeartDisease
    GlucoseLevel -> Stroke
    Work -> Residence
    Work -> BMI
    Work -> Hypertension
    Married -> Residence
    Married -> BMI
    Married -> HeartDisease
    Married -> Stroke

    Married <-> Smoking
    Married <-> Work
    BMI <-> GlucoseLevel
}