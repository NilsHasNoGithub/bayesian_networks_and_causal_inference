dag {
    WorkMarried[latent]
    GlucoseLevelBMI[latent]
    SmokingMarried[latent]
    Age
    Hypertension
    HeartDisease
    Gender
    BMI
    GlucoseLevel
    Work
    Residence
    Married
    Smoking
    Stroke
    Age -> Hypertension
    Age -> HeartDisease
    Age -> BMI
    Age -> Stroke
    Age -> Smoking
    Age -> Married
    Age -> Work
    Age -> GlucoseLevel
    Gender -> Hypertension
    Gender -> HeartDisease
    Gender -> Work
    Gender -> BMI
    Gender -> Smoking
    Gender -> Stroke
    Hypertension -> HeartDisease
    Hypertension -> Stroke
    HeartDisease -> Stroke
    Smoking -> Hypertension
    Smoking -> HeartDisease
    Smoking -> BMI
    Smoking -> Married
    BMI -> Hypertension
    BMI -> HeartDisease
    BMI -> GlucoseLevel
    BMI -> Stroke
    GlucoseLevel -> BMI
    GlucoseLevel -> Hypertension
    GlucoseLevel -> HeartDisease
    GlucoseLevel -> Stroke
    Work -> Residence
    Work -> BMI
    Work -> Hypertension
    Married -> Residence
    Married -> BMI
    Married -> HeartDisease
    Married -> Stroke
    Married -> Smoking
    WorkMarried -> Work
    WorkMarried -> Married
    GlucoseLevelBMI -> GlucoseLevel
    GlucoseLevelBMI -> BMI
    SmokingMarried -> Smoking
    SmokingMarried -> Married
}